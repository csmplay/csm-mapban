{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "outputs": []
    },
    "start": {}
  },
  "globalEnv": ["FRONTEND_URL", "NEXT_PUBLIC_CDN_BASE" ,"NEXT_PUBLIC_CDN_LOGO"]
}
